<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" circular autoplay indicator-dots interval="2000" duration="300" indicator-active-color="#ff5777">
  <swiper-item class="swiper-item" wx:for="{{list}}" wx:key="index">
    <image src="{{item}}" mode="widthFix"></image>
  </swiper-item>
</swiper>
