/* tab-control/tab-control.wxss */
.tab-control{
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  background-color: white;
  padding-bottom: 8rpx;
}
.tab-item{
  flex: 1;
  text-align: center
}
.active{
  color: #ff5777;
}
.active text{
  padding: 10rpx 16rpx;
  border-bottom: 6rpx solid #ff5777
}